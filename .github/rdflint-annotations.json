{
	"problemMatcher": [
		{
			"owner": "rdflint",
			"severity": "warning",
			"pattern": [
				{
					"regexp": "^([^\\s]*\\.(rdf|ttl|xml)+)$",
					"file": 1
				},
				{
					"regexp": "^\\s+(ERROR|WARN|INFO)\\s+([^(]*\\(line:\\s*([\\d+]+).*)$",
					"line": 2,
					"message": 3,
					"loop": true
				}
			]
		}
	]
}
